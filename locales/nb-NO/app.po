msgctxt "search_add_manually_name_hint"
msgid "Search engine name"
msgstr "Søkemotornavn"

msgctxt "search_add_manually_string"
msgid "Search string to use"
msgstr "Søkestreng å bruke"

msgctxt "search_add_manually_save"
msgid "Save"
msgstr "Lagre"

